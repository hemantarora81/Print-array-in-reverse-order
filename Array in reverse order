package com.company;

public class Main {
   
    
    public static void main(String[] args) {
        //Print array in reverse order
        int [] arr = {12,13,14,15};
        System.out.println("Reversing Array");
        int length = arr.length;
        for (int i = arr.length-1;i>=0;i--){
            System.out.println(arr[i]);
        }
}
}
